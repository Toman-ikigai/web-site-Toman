---
import "@/styles/globals.css";
import Layout from "../layouts/Layout.astro";
import Header from "../components/Header.astro";
import Carousel from "../components/Carousel";
import ServicesSection from "@/components/section/ServicesSection";
import { Servicio } from "@/components/Servicio";
import MainServiceAreas from "@/components/section/MainServiceAreas";
import AboutUs from "../components/AboutUs.astro";
import Projects from "../components/Projects.astro";
import Catalog from "../components/Catalog.astro";
import LatestFinishedProjects from "@/components/section/LatestFinishedProjects";
import MissionandVision from "@/components/section/MissionandVision.astro";
import Footer from "@/components/Footer";
---

<Layout title="Integrator Company">
  <Header />
  <main>
    <Carousel client:load />
    <ServicesSection />
    <Servicio client:load />
    <MainServiceAreas />
    <!-- <AboutUs /> -->
    <!-- <Projects /> -->
    <LatestFinishedProjects client:visible />
    <Catalog />
    <MissionandVision />
  </main>
  <Footer />
</Layout>
